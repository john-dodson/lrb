<h1>Listing venue_to_vendors</h1>

<table>
  <thead>
    <tr>
      <th>Venue</th>
      <th>Vendor</th>
      <th>Vendor type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venue_to_vendors.each do |venue_to_vendor| %>
      <tr>
        <td><%= venue_to_vendor.venue_id %></td>
        <td><%= venue_to_vendor.vendor_id %></td>
        <td><%= venue_to_vendor.vendor_type_id %></td>
        <td><%= link_to 'Show', venue_to_vendor %></td>
        <td><%= link_to 'Edit', edit_venue_to_vendor_path(venue_to_vendor) %></td>
        <td><%= link_to 'Destroy', venue_to_vendor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Venue to vendor', new_venue_to_vendor_path %>
