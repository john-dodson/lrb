<h1>Listing caterer_dates</h1>

<table>
  <thead>
    <tr>
      <th>Caterer</th>
      <th>Date</th>
      <th>Service</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @caterer_dates.each do |caterer_date| %>
      <tr>
        <td><%= caterer_date.caterer_id %></td>
        <td><%= caterer_date.date %></td>
        <td><%= caterer_date.service_id %></td>
        <td><%= link_to 'Show', caterer_date %></td>
        <td><%= link_to 'Edit', edit_caterer_date_path(caterer_date) %></td>
        <td><%= link_to 'Destroy', caterer_date, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Caterer date', new_caterer_date_path %>
